{
  "type": "Menu",
  "title": {
    "name": "音频播放",
    "size": 15,
    "elevation": 3,
    "background": "#7581FF",
    "padding": [4, 4, 4, 4],
    "colors": ["#FF0000", "#0000FF"],
    "image_scaled": true
  },
  "color": "#FEFEFE",
  "alpha": 0.85,
  "can_close": true,
  "image_scaled_size": 40,
  "radius": 10,
  "show_dividers": false,
  "hide": true,
  "items": [
    {
      "type": "TextView",
      "html": "<a href='https://github.com/Prejudice-Studio/AsakaMod-36D-SoundEffect'>项目地址</a>",
      "size": 12,
      "padding": [10, 5, 10, 5]
    },
    {
      "type": "TextView",
      "html": "UI原作者：<a href='https://qm.qq.com/cgi-bin/qm/qr?k=yMgA1MGNCZVR5qZhI5BzHzO0dWA3hwg5'>Lossiay</a>",
      "color": "#BC8F8F",
      "size": 12,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "name": ">36D音频播放<",
      "color": "#8CFFE4",
      "size": 12,
      "padding": [5, 5, 5, 5],
      "load_script": "36D音频播放.js"
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "name": "退出脚本",
      "color": "#000000",
      "size": 11,
      "key": "exit",
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "Switch",
      "on_sound": "on.mp3",
      "off_sound": "off.mp3",
      "key": "soundPlayerEnabled",
      "name": "8D音效",
      "color": "#000000",
      "size": 11,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "soundAdd",
      "name": "类型ID + 1",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "CheckBox",
      "on_sound": "√.mp3",
      "off_sound": "×.mp3",
      "key": "surroundAllPlayers",
      "name": "全部玩家",
      "color": "#63bbd0",
      "size": 10,
      "checked": true,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "CheckBox",
      "on_sound": "√.mp3",
      "off_sound": "×.mp3",
      "key": "surroundAllEntities",
      "name": "全部实体",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "CheckBox",
      "on_sound": "√.mp3",
      "off_sound": "×.mp3",
      "key": "surroundExcludeSelf",
      "name": "排除自己",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "CheckBox",
      "on_sound": "√.mp3",
      "off_sound": "×.mp3",
      "key": "surroundLoop",
      "name": "循环播放",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "soundType",
      "name": "编辑音效类型",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "soundLevel",
      "name": "编辑音效音调",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "addPlayPos",
      "name": "添加播放坐标",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "deletePlayPos",
      "name": "清空播放坐标",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "loadSound",
      "name": "加载音频文件",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "TextView",
      "sound": "click.mp3",
      "key": "selectSound",
      "name": "选择音频文件",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5]
    },
    {
      "type": "SeekBar",
      "key": "soundDistance",
      "format": "环绕距离:%d",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5],
      "value": 1,
      "min": 1,
      "max": 10
    },
    {
      "type": "SeekBar",
      "key": "soundRepeat",
      "format": "播放次数:%d",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5],
      "value": 1,
      "min": 1,
      "max": 500
    },
    {
      "type": "SeekBar",
      "key": "soundVector",
      "format": "播放倍速:%d",
      "color": "#63bbd0",
      "size": 10,
      "padding": [5, 5, 5, 5],
      "value": 1,
      "min": 1,
      "max": 10
    }
  ]
}
