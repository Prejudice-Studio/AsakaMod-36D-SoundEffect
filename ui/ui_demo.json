    },
    {
      "type": "TextView",
      "name": "测试功能-点击",
      "color": "$menu_item_color",
      "size": 10,
      "padding": [
        10,
        5,
        10,
        5
      ],
      "tag": "fun_custom_02",
      "warning": "可以与脚本交互,在ui_definition注册功能tag"
    }
