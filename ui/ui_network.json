{
  "type": "Menu",
  "title": {
    "name": "网络类",
    "size": 15,
    "elevation": 3,
    "background": "$menu_title_background_color",
    "padding": [
      4,
      4,
      4,
      4
    ],
    "colors": [
      "$menu_title_gradient_text_begin_color",
      "$menu_title_gradient_text_end_color"
    ],
    "image_scaled": true
  },
  "color": "$menu_color",
  "alpha": 0.85,
  "can_close": true,
  "image_scaled_size": 40,
  "radius": 10,
  "show_dividers": false,
  "hide": true,
  "items": [
    {
      "type": "Switch",
      "name": "重复数据包",
      "color": "$menu_item_color",
      "size": 10,
      "padding": [
        5,
        0,
        5,
        0
      ],
      "tag": "fun_multiple_packet",
      "items": [
        {
          "type": "SeekBar",
          "key": "count",
          "format": "数量%d",
          "color": "$menu_item_color",
          "size": 8,
          "padding": [
            0,
            0,
            0,
            0
          ],
          "value": 50,
          "min": 1,
          "max": 100
        }
      ]
    },
    {
      "type": "Switch",
      "name": "停止数据包",
      "color": "$menu_item_color",
      "size": 10,
      "padding": [
        5,
        0,
        5,
        0
      ],
      "tag": "fun_no_packet"
    },
    {
      "type": "TextView",
      "name": "网络数据包管理+",
      "color": "$menu_item_color",
      "size": 10,
      "padding": [
        5,
        5,
        5,
        5
      ],
      "tag": "fun_network_packet_mgr"
    }
  ]
}